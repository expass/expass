{
  "compilerOptions": {
    "lib": ["ES2023"],
    "target": "ES2022",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@expass/core": ["packages/core/src"]
    }
  }
}
